    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.3.3",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "execa": "^7.1.1",
    "glob": "^10.3.1",
    "jszip": "^3.10.1",
    "minimatch": "^9.0.3",
    "pretty-bytes": "^6.1.0",
