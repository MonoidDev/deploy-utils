{
  "name": "@monoid-dev/lambda-s3-builder",
  "version": "0.0.9",
  "keywords": [],
  "type": "commonjs",
  "bin": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "src/",
    "lib/"
  ],
  "author": "Chenyu Wang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MonoidDev/deploy-utils"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "prepare": "pnpm build",
    "build": "tsup src/index.ts --format esm,cjs --splitting -d lib/ && tsc -p tsconfig.build.json",
    "lint": "tsc && eslint ./src --ext .ts,.tsx",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.363.0",
    "@aws-sdk/client-s3": "^3.363.0",
    "@monoid-dev/deploy-utils-models": "workspace:^",
    "@types/node": "^20.3.3",
    "chalk": "^5.3.0",
    "execa": "^7.1.1",
    "glob": "^10.3.1",
    "jszip": "^3.10.1",
    "pretty-bytes": "^6.1.0",
    "remeda": "^1.23.0",
    "zod": "^3.21.4"
  }
}
